{
    "name": "sparked",
    "displayName": "sparked",
    "description": "sparked is an extension that helps you adopt our design system units faster",
    "version": "0.0.7",
    "publisher": "majideltayeb",
    "repository": "https://github.com/majidzeno/sparked",
    "icon": "images/sparked-logo.png",
    "main": "./extension.js",
    "activationEvents": [
        "onCommand:sparked.showSnippets"
    ],
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "commands": [
            {
                "command": "sparked.showSnippets",
                "title": "Show Sparked Snippets",
                "category": "Sparked"
            }
        ],
        "keybindings": [
            {
                "command": "sparked.showSnippets",
                "key": "shift+tab",
                "when": "editorTextFocus"
            }
        ],
        "snippets": [
            {
                "language": "",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "",
                "path": "./snippets/snippets-utils.code-snippets"
            }
        ]
    }
}
